<configuration>
  <system.webServer>
    <handlers>
      <add name="iisnode" path="static.js" verb="*" modules="iisnode" />
    </handlers>
    <rewrite>
			 <rules>
				 <rule name="static">
					 <match url="/*" />
					 <action type="Rewrite" url="static.js" />
				 </rule>
			 </rules>
		</rewrite>
  </system.webServer>
</configuration>